<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools" android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context="org.itech.vmmc.AddEditPersonFragment">

    <ImageView
        android:id="@+id/imageView1"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:src="@drawable/itech50" />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginLeft="10dp"
        android:textSize="@dimen/font_size_small"
        android:text="@string/Username"
        android:id="@+id/textViewUsername"
        android:paddingTop="128dp"
        android:layout_below="@+id/imageViewVMMC" />

    <org.itech.vmmc.ClearableAutoCompleteTextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:textSize="@dimen/font_size_small"
        android:id="@+id/username"
        android:layout_marginLeft="200dp"
        android:layout_marginRight="50dp"
        android:layout_alignParentEnd="true"
        android:layout_alignParentStart="true"
        android:layout_toEndOf="@+id/textViewUsername"
        android:layout_below="@+id/imageViewVMMC"
        android:paddingTop="128dp"
        android:maxLines="1" />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginLeft="10dp"
        android:textSize="@dimen/font_size_small"
        android:text="@string/phone"
        android:id="@+id/textViewPhone"
        android:paddingTop="12dp"
        android:layout_below="@+id/textViewUsername" />

    <EditText
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:textSize="@dimen/font_size_small"
        android:id="@+id/phone"
        android:layout_marginLeft="200dp"
        android:layout_marginRight="50dp"
        android:layout_alignParentEnd="true"
        android:layout_alignParentStart="true"
        android:layout_toEndOf="@+id/textViewPhone"
        android:layout_below="@+id/textViewUsername"
        android:maxLines="1" />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginLeft="10dp"
        android:textSize="@dimen/font_size_small"
        android:text="@string/password"
        android:id="@+id/textViewPassword"
        android:paddingTop="12dp"
        android:layout_below="@+id/textViewPhone" />

    <EditText
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:textSize="@dimen/font_size_small"
        android:id="@+id/password"
        android:layout_marginLeft="200dp"
        android:layout_marginRight="50dp"
        android:layout_alignParentEnd="true"
        android:layout_alignParentStart="true"
        android:layout_toEndOf="@+id/textViewPassword"
        android:layout_below="@+id/textViewPhone"
        android:inputType="textPassword"
        android:maxLines="1" />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginLeft="10dp"
        android:textSize="@dimen/font_size_small"
        android:text="@string/constituency"
        android:id="@+id/textViewLocation"
        android:paddingTop="12dp"
        android:layout_below="@+id/textViewPassword" />

    <Spinner
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/vmmclocation"
        android:textSize="@dimen/font_size_small"
        android:layout_marginLeft="200dp"
        android:layout_marginRight="50dp"
        android:layout_alignParentEnd="true"
        android:layout_alignParentStart="true"
        android:layout_toEndOf="@+id/textViewLocation"
        android:paddingTop="12dp"
        android:layout_below="@+id/textViewPassword" />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginLeft="10dp"
        android:textSize="@dimen/font_size_small"
        android:text="@string/Usergroup"
        android:id="@+id/textViewUsergroup"
        android:paddingTop="12dp"
        android:layout_below="@+id/textViewLocation" />

    <Spinner
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/user_type"
        android:textSize="@dimen/font_size_small"
        android:layout_marginLeft="200dp"
        android:layout_marginRight="50dp"
        android:layout_alignParentEnd="true"
        android:layout_alignParentStart="true"
        android:layout_toEndOf="@+id/textViewUsergroup"
        android:paddingTop="12dp"
        android:layout_below="@+id/textViewLocation" />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginLeft="10dp"
        android:textSize="@dimen/font_size_small"
        android:text="@string/first_name"
        android:id="@+id/textViewFirstName"
        android:paddingTop="12dp"
        android:layout_below="@+id/textViewUsergroup" />

    <EditText
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:textSize="@dimen/font_size_small"
        android:id="@+id/first_name"
        android:layout_marginLeft="200dp"
        android:layout_marginRight="50dp"
        android:layout_alignParentEnd="true"
        android:layout_alignParentStart="true"
        android:layout_toEndOf="@+id/textViewFirstName"
        android:layout_below="@+id/textViewUsergroup"
        android:maxLines="1" />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginLeft="10dp"
        android:textSize="@dimen/font_size_small"
        android:text="@string/last_name"
        android:id="@+id/textViewLastName"
        android:paddingTop="12dp"
        android:layout_below="@+id/textViewFirstName" />

    <EditText
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:textSize="@dimen/font_size_small"
        android:id="@+id/last_name"
        android:layout_marginLeft="200dp"
        android:layout_marginRight="50dp"
        android:layout_alignParentEnd="true"
        android:layout_alignParentStart="true"
        android:layout_toEndOf="@+id/textViewLastName"
        android:layout_below="@+id/textViewFirstName"
        android:maxLines="1" />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginLeft="10dp"
        android:textSize="@dimen/font_size_small"
        android:text="@string/email"
        android:id="@+id/textViewEmail"
        android:paddingTop="12dp"
        android:layout_below="@+id/textViewLastName" />

    <EditText
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:textSize="@dimen/font_size_small"
        android:id="@+id/email"
        android:layout_marginLeft="200dp"
        android:layout_marginRight="50dp"
        android:layout_alignParentEnd="true"
        android:layout_alignParentStart="true"
        android:layout_toEndOf="@+id/textViewEmail"
        android:layout_below="@+id/textViewLastName"
        android:maxLines="1" />


    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginLeft="10dp"
        android:textSize="@dimen/font_size_small"
        android:text="@string/Region"
        android:id="@+id/textViewRegion"
        android:paddingTop="12dp"
        android:layout_below="@+id/textViewEmail" />



        <CheckBox
            android:layout_marginLeft="125dp"
            android:layout_width="100sp"
            android:id="@+id/cb_Region1"
            android:layout_height="wrap_content"
            android:textSize="@dimen/font_size_small"
            android:text="@string/Oshana"
            android:textAlignment="viewStart"
            android:layout_toEndOf="@+id/textViewRegion"
            android:layout_below="@+id/email" />

        <CheckBox
            android:layout_width="125sp"
            android:id="@+id/cb_Region2"
            android:layout_height="wrap_content"
            android:textSize="@dimen/font_size_small"
            android:text="@string/Zambezi"
            android:textAlignment="textEnd"
            android:layout_toEndOf="@+id/cb_Region1"
            android:layout_below="@+id/email" />



    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginLeft="10dp"
        android:textSize="@dimen/font_size_small"
        android:text="@string/is_blocked"
        android:id="@+id/textViewIsBlocked"
        android:paddingTop="12dp"
        android:layout_below="@+id/textViewRegion" />

    <RadioGroup
        android:layout_marginLeft="200dp"
        android:layout_marginRight="50dp"
        android:layout_alignParentEnd="true"
        android:layout_alignParentStart="true"
        android:layout_width="fill_parent"
        android:layout_height="wrap_content"
        android:id="@+id/is_blocked_radio_group"
        android:textSize="@dimen/font_size_small"
        android:layout_toEndOf="@+id/textViewIsBlocked"
        android:layout_below="@+id/textViewRegion"
        android:orientation="horizontal"
        android:paddingTop="12dp" >

        <RadioButton
            android:layout_width="100sp"
            android:id="@+id/radioButtonNo"
            android:layout_height="wrap_content"
            android:textSize="@dimen/font_size_small"
            android:text="No"
            android:textAlignment="viewStart" />

        <RadioButton
            android:layout_width="100sp"
            android:id="@+id/radioButtonYes"
            android:layout_height="wrap_content"
            android:textSize="@dimen/font_size_small"
            android:text="Yes"
            android:textAlignment="textEnd" />

    </RadioGroup>

    <Button
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:textSize="@dimen/font_size_small"
        android:text="@string/btn_label_update"
        android:id="@+id/btnUpdate"
        android:layout_below="@+id/textViewIsBlocked"
        android:layout_alignEnd="@+id/user_type"
        android:paddingTop="12dp"
        android:layout_marginTop="32dp" />

